{
  "name": "@web-uni/uni-ui",
  "description": "UI components for react.js websites and apps",
  "author": "unicorn-84 <unicorn-84@ya.ru> (https://github.com/unicorn-84)",
  "license": "MIT",
  "title": "Uni UI",
  "main": "build/index.js",
  "keywords": [
    "@web-uni",
    "uni-ui",
    "react",
    "components",
    "ui"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unicorn-84/uni-ui.git"
  },
  "bugs": {
    "url": "https://github.com/unicorn-84/uni-ui/issues"
  },
  "scripts": {
    "start": "webpack serve --mode development",
    "prepublishOnly": "rm -rdf build && babel src -d build --ignore src/setupTest.js",
    "lint:js": "eslint ./src --ext .js,.jsx --fix",
    "lint:css": "stylelint './src/**/*.css' --fix",
    "lint": "npm run lint:js && npm run lint:css",
    "preversion": "npm run lint && npm test",
    "postversion": "git push && git push --tags",
    "test:watch": "jest -c jest.config.js --watch",
    "test": "jest -c jest.config.js"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.17.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^4.5.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "style-loader": "^2.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "webpack": "^5.13.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.2"
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/react@1.0.21"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "version": "0.0.2"
}
